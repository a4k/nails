{
  "name": "nails",
  "version": "1.0.0",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore"
  },
  "jest": {
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "text-summary",
      "clover"
    ],
    "collectCoverageFrom": [
      "./src/**/*.{js,jsx,ts,tsx}",
      "!./node_modules/",
      "!./build/"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.9.0",
